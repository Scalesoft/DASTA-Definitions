<?xml version="1.0" encoding="UTF-8"?>
<!-- DTD pro predavani zdravotnickych dat v XML -->
<!-- ds031001.dtd 08.12.2006 [PH]   NZIS-2004,5,6,LAB -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <xs:attributeGroup name="aktualizace_bloku">
    <xs:attribute name="dat_ab"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="vytvoreni_bloku">
    <xs:attribute name="dat_vb" use="required"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="vytvoreni_bl_imp">
    <xs:attribute name="dat_vb"/>
  </xs:attributeGroup>
  <xs:simpleType name="s_hodnota">
    <xs:restriction base="xs:token">
      <xs:enumeration value="-4"/>
      <xs:enumeration value="-3"/>
      <xs:enumeration value="-2"/>
      <xs:enumeration value="-1"/>
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="t_hodnota">
    <xs:restriction base="xs:token">
      <xs:enumeration value="-3"/>
      <xs:enumeration value="-2"/>
      <xs:enumeration value="-1"/>
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:attributeGroup name="opravne_sdeleni">
    <xs:attribute name="ind_oprav_sd" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="N"/>
          <xs:enumeration value="E"/>
          <xs:enumeration value="S"/>
          <xs:enumeration value="U"/>
          <xs:enumeration value="Z"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="sr_ident_1">
    <xs:attribute name="sr_typ" type="xs:NMTOKEN"/>
    <xs:attribute name="sr_pois" type="xs:NMTOKEN"/>
    <xs:attribute name="sr_kod" type="xs:NMTOKEN"/>
    <xs:attribute name="sr_lekar" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="sr_ident_2">
    <xs:attribute name="sr_cito" type="xs:NMTOKEN"/>
    <xs:attribute name="sr_kod_lek" type="xs:NMTOKEN"/>
    <xs:attribute name="sr_mnozstvi" type="xs:NMTOKEN"/>
    <xs:attribute name="sr_cena" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="sr_ident_3">
    <xs:attribute name="sr_nahrady" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <!-- ciselniky NZIS in alphabetical order -->
  <!-- ciselnik NZIS ATBDRUH - seznam hodnot -->
  <!-- ciselnik NZIS ANAMNEZA -->
  <!-- ciselnik NZIS ANONE - seznam hodnot -->
  <!-- ciselnik NZIS ANTIKO - seznam hodnot -->
  <!-- ciselnik NZIS ASREPR - seznam hodnot -->
  <!-- ciselnik NZIS CETTEH - seznam hodnot -->
  <!-- ciselnik NZIS DISSKU - seznam hodnot -->
  <!-- ciselnik NZIS DOPHOSP - seznam hodnot -->
  <xs:simpleType name="nzcis_dophosp">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS DROPER - seznam hodnot -->
  <xs:simpleType name="nzcis_droper">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS DRUHLA - seznam hodnot -->
  <!-- ciselnik NZIS DRUPOT- seznam hodnot -->
  <!-- ciselnik NZIS DRVLA - seznam hodnot -->
  <!-- ciselnik NZIS DUPRIJ - seznam hodnot -->
  <xs:simpleType name="nzcis_duprij">
    <xs:restriction base="xs:token">
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="7"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS DUVNEPR - seznam hodnot -->
  <!-- ciselnik NZIS DUVUK - seznam hodnot -->
  <!-- ciselnik NZIS DUVVYS - seznam hodnot -->
  <!-- ciselnik NZIS EAKTIV - seznam hodnot -->
  <!-- ciselnik NZIS ETNIKA - seznam hodnot -->
  <!-- ciselnik NZIS EVIPRO - seznam hodnot -->
  <!-- ciselnik NZIS GRAVID - seznam hodnot -->
  <!-- ciselnik NZIS HIST - seznam hodnot -->
  <!-- ciselnik NZIS HISVYS - seznam hodnot -->
  <!-- ciselnik NZIS HIV - seznam hodnot -->
  <!-- ciselnik NZIS HOSPRV - seznam hodnot -->
  <xs:simpleType name="nzcis_hosprv">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS KOURENI -->
  <!-- ciselnik NZIS KOMBIN - seznam hodnot -->
  <!-- ciselnik NZIS KZAM - seznam hodnot -->
  <xs:simpleType name="nzcis_kzam">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS LATERAL (lateralita) -->
  <!-- ciselnik NZIS LEC - seznam hodnot -->
  <!-- ciselnik NZIS METODA - seznam hodnot -->
  <!-- ciselnik NZIS MYKMAT - seznam hodnot -->
  <!-- ciselnik NZIS NARODN - seznam hodnot -->
  <xs:simpleType name="nzcis_narodn">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS NEMNAK - seznam hodnot -->
  <xs:simpleType name="nzcis_nemnak">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <!--  ciselnik NZIS NELPRO (nelecen pro) -->
  <!-- ciselnik NZIS OCKTBC - seznam hodnot -->
  <!-- ciselnik NZIS ODSTRNAD -->
  <!-- ciselnik NZIS ONEMOC -->
  <!-- ciselnik NZIS PITVA pro NOR -->
  <!-- ciselnik NZIS PODAPE - seznam hodnot -->
  <xs:simpleType name="nzcis_podape">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS POHLAV - seznam hodnot -->
  <!-- ciselnik NZIS POLOHA - seznam hodnot -->
  <!-- ciselnik NZIS POOPKO - seznam hodnot -->
  <xs:simpleType name="nzcis_poopko">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS POUPT - seznam hodnot -->
  <!-- ciselnik NZIS PRENDIAG - seznam hodnot -->
  <!-- ciselnik NZIS PRIJ - seznam hodnot -->
  <xs:simpleType name="nzcis_prij">
    <xs:restriction base="xs:token">
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS POROD - seznam hodnot -->
  <!-- ciselnik NZIS ZPUSOB - seznam hodnot -->
  <!-- ciselnik NZIS RANES - seznam hodnot -->
  <!-- ciselnik NZIS RDSTAV - seznam hodnot -->
  <!-- ciselnik NZIS REOPER - seznam hodnot -->
  <xs:simpleType name="nzcis_reoper">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS REZORTM - seznam hodnot -->
  <!-- ciselnik NZIS RDUVUK - seznam hodnot -->
  <!-- ciselnik NZIS ZRIZ - seznam hodnot -->
  <!-- ciselnik NZIS RODHAD - seznam hodnot -->
  <!-- ciselnik NZIS ROSTAV - seznam hodnot -->
  <xs:simpleType name="nzcis_rostav">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS RPLOD - seznam hodnot -->
  <!-- ciselnik NZIS RSC - seznam hodnot -->
  <!-- ciselnik NZIS RTG - seznam hodnot -->
  <!-- ciselnik NZIS RTGNAL - seznam hodnot -->
  <!-- ciselnik NZIS RVAGIN - seznam hodnot -->
  <!-- ciselnik NZIS RVEDL - seznam hodnot -->
  <!-- ciselnik NZIS RZHOD - seznam hodnot -->
  <!-- ciselnik NZIS SEXZAM - seznam hodnot -->
  <!-- ciselnik NZIS SOCPOST -->
  <!-- ciselnik NZIS OBCAN - seznam hodnot -->
  <!-- ciselnik NZIS STAV -->
  <!-- ciselnik NZIS UKHOSP - seznam hodnot -->
  <xs:simpleType name="nzcis_ukhosp">
    <xs:restriction base="xs:token">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS UMRTI - seznam hodnot -->
  <!-- ciselnik NZIS VITALITA - seznam hodnot -->
  <!-- ciselnik NZIS VVVROD - seznam hodnot -->
  <!-- ciselnik NZIS VYSETR - seznam hodnot -->
  <!-- ciselnik NZIS VYSHIV - seznam hodnot -->
  <!-- ciselnik NZIS VYSKYT - seznam hodnot -->
  <!-- ciselnik NZIS VYSLTEH - seznam hodnot -->
  <!-- ciselnik NZIS VYZIV - seznam hodnot -->
  <!-- ciselnik NZIS VZDELA - seznam hodnot -->
  <xs:simpleType name="nzcis_vzdela">
    <xs:restriction base="xs:token">
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ciselnik NZIS RVZDELA - seznam hodnot -->
  <!-- ciselnik NZIS ZAHL - seznam hodnot -->
  <!-- ciselnik NZIS ZDROJ - seznam hodnot -->
  <!-- ciselnik NZIS ZDUPT - seznam hodnot -->
  <!-- ciselnik NZIS ZJIPRI -->
  <!-- ciselnik NZIS ZJISTENO - seznam hodnot -->
  <!-- ciselnik NZIS ZJITBC - seznam hodnot -->
  <!-- hlavni/korenovy element -->
  <!-- je dodrzena strategie jeden prijemce, vice odesilatelu, vice pacientu -->
  <xs:element name="dasta">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="zdroj_is"/>
        <xs:element ref="pm"/>
        <xs:element minOccurs="0" ref="garant_dat"/>
        <xs:choice>
          <xs:element maxOccurs="unbounded" ref="is"/>
          <xs:element ref="pd"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="prot_kom"/>
      </xs:sequence>
      <xs:attribute name="id_soubor" use="required"/>
      <xs:attribute name="verze_ds" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="verze_nclp" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="bin_priloha" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="T"/>
            <xs:enumeration value="B"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ur" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="R"/>
            <xs:enumeration value="S"/>
            <xs:enumeration value="U"/>
            <xs:enumeration value="V"/>
            <xs:enumeration value="B"/>
            <xs:enumeration value="C"/>
            <xs:enumeration value="H"/>
            <xs:enumeration value="T"/>
            <xs:enumeration value="N"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="typ_odesm" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="ozn_soub"/>
      <xs:attribute name="potvrzeni">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="N"/>
            <xs:enumeration value="P"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="vytvoreni_bloku"/>
    </xs:complexType>
  </xs:element>
  <!-- informace o firme a IS kterym byl soubor vytvoren -->
  <xs:element name="zdroj_is">
    <xs:complexType>
      <xs:attribute name="kod_firmy" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="kod_prog" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="verze_prog" type="xs:NMTOKEN"/>
      <xs:attribute name="liccis_prog"/>
    </xs:complexType>
  </xs:element>
  <!--
    protokol o komunikaci - blok se pripravuje a neni doreseno 
    jeho pouzivani ani obsah
  -->
  <xs:element name="prot_kom">
    <xs:complexType>
      <xs:attribute name="pd_n">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="A"/>
            <xs:enumeration value="E"/>
            <xs:enumeration value="N"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- prijmove misto -->
  <xs:element name="pm">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="as"/>
        <xs:element minOccurs="0" ref="a"/>
      </xs:sequence>
      <xs:attribute name="ico" type="xs:NMTOKEN"/>
      <xs:attribute name="icz" type="xs:NMTOKEN"/>
      <xs:attribute name="icp" type="xs:NMTOKEN"/>
      <xs:attribute name="icl" type="xs:NMTOKEN"/>
      <xs:attribute name="pcz" type="xs:NMTOKEN"/>
      <xs:attribute name="oddel" type="xs:NMTOKEN"/>
      <xs:attributeGroup ref="sr_ident_1"/>
    </xs:complexType>
  </xs:element>
  <!-- odesilatel -->
  <xs:element name="is">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="as"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="os"/>
        <xs:element minOccurs="0" ref="a"/>
        <xs:choice>
          <xs:element maxOccurs="unbounded" ref="ip"/>
          <xs:element maxOccurs="unbounded" ref="idu"/>
          <xs:element maxOccurs="unbounded" ref="ivv"/>
          <xs:element maxOccurs="unbounded" ref="ilb"/>
          <xs:element maxOccurs="unbounded" ref="ilc"/>
          <xs:element maxOccurs="unbounded" ref="ihe"/>
          <xs:element maxOccurs="unbounded" ref="text"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="ico" type="xs:NMTOKEN"/>
      <xs:attribute name="icz" type="xs:NMTOKEN"/>
      <xs:attribute name="icp" type="xs:NMTOKEN"/>
      <xs:attribute name="icl" type="xs:NMTOKEN"/>
      <xs:attribute name="pcz" type="xs:NMTOKEN"/>
      <xs:attribute name="oddel" type="xs:NMTOKEN"/>
      <xs:attribute name="oavl" type="xs:NMTOKEN"/>
      <xs:attributeGroup ref="sr_ident_1"/>
    </xs:complexType>
  </xs:element>
  <!-- potvrzeni doruceni souboru -->
  <xs:element name="pd">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="chyba_pd"/>
        <xs:element ref="as"/>
        <xs:element minOccurs="0" ref="dat_os"/>
        <xs:element ref="dat_ps"/>
      </xs:sequence>
      <xs:attribute name="id_soubor" use="required"/>
      <xs:attribute name="stav" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="A"/>
            <xs:enumeration value="N"/>
            <xs:enumeration value="O"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- chybove hlaseni - popis chyby v prijatem souboru -->
  <xs:element name="chyba_pd">
    <xs:complexType mixed="true">
      <xs:attribute name="kod" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="lokalizace"/>
      <xs:attribute name="osetreni"/>
      <xs:attribute name="popis"/>
    </xs:complexType>
  </xs:element>
  <!-- adresa -->
  <xs:element name="a">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="dat_od"/>
        <xs:element minOccurs="0" ref="dat_do"/>
        <xs:element ref="jmeno"/>
        <xs:element minOccurs="0" ref="adr"/>
        <xs:element minOccurs="0" ref="dop1"/>
        <xs:element minOccurs="0" ref="dop2"/>
        <xs:element minOccurs="0" ref="psc"/>
        <xs:element minOccurs="0" ref="mesto"/>
        <xs:element minOccurs="0" ref="stat"/>
        <xs:element minOccurs="0" ref="gps"/>
        <xs:element minOccurs="0" ref="vztah"/>
        <xs:element minOccurs="0" ref="icl"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="as"/>
      </xs:sequence>
      <xs:attribute name="typ" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="5"/>
            <xs:enumeration value="6"/>
            <xs:enumeration value="7"/>
            <xs:enumeration value="8"/>
            <xs:enumeration value="9"/>
            <xs:enumeration value="L"/>
            <xs:enumeration value="A"/>
            <xs:enumeration value="S"/>
            <xs:enumeration value="B"/>
            <xs:enumeration value="P"/>
            <xs:enumeration value="O"/>
            <xs:enumeration value="X"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ind_kont">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="K"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="sr_ident_1"/>
    </xs:complexType>
  </xs:element>
  <!-- adresa spojeni -->
  <xs:element name="as">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="obsah"/>
        <xs:element minOccurs="0" ref="vnitrni"/>
        <xs:element minOccurs="0" ref="sdeleni"/>
      </xs:sequence>
      <xs:attribute name="typ" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="D"/>
            <xs:enumeration value="T"/>
            <xs:enumeration value="F"/>
            <xs:enumeration value="S"/>
            <xs:enumeration value="X"/>
            <xs:enumeration value="M"/>
            <xs:enumeration value="E"/>
            <xs:enumeration value="I"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="poradi" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <!-- gps -->
  <xs:element name="gps">
    <xs:complexType>
      <xs:attribute name="lat" use="required"/>
      <xs:attribute name="lon" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- datum a cas -->
  <xs:element name="dat_ak">
    <xs:complexType mixed="true">
      <xs:attribute name="format">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="DTS"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="dat_de">
    <xs:complexType mixed="true">
      <xs:attribute name="format">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="D"/>
            <xs:enumeration value="DT"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="dat_do">
    <xs:complexType mixed="true">
      <xs:attribute name="format">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="D"/>
            <xs:enumeration value="DT"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="dat_dn">
    <xs:complexType mixed="true">
      <xs:attribute name="format">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="D"/>
            <xs:enumeration value="DT"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="dat_ds">
    <xs:complexType mixed="true">
      <xs:attribute name="format">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="D"/>
            <xs:enumeration value="DTS"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
