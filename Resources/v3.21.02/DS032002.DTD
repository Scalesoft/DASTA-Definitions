<!ENTITY % aktualizace_bloku
       "dat_ab CDATA  #IMPLIED" >
<!ENTITY % vytvoreni_bloku
       "dat_vb CDATA  #REQUIRED" >
<!ENTITY % vytvoreni_bl_imp
       "dat_vb CDATA  #IMPLIED" >
<!ENTITY % s_hodnota
"(-4|-3|-2|-1|0|1|2|3|4)" >
